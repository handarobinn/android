<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Test"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="30" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label=" Test ToolBar"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.Test" >
        <activity
            android:name="com.Test.TestToolBar"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name="com.Test.EmptyActivity" />
        <activity android:name="com.Test.WeatherForecast" />
        <activity
            android:name="com.Test.ProfileActivity"
            android:configChanges="locale" />
        <activity
            android:name="com.Test.ChatRoomActivity"
            android:configChanges="locale" />
        <activity
            android:name="com.Test.MainActivity"
            android:configChanges="locale" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>